    "@claviska/jquery-minicolors": "^2.2.4",
    "@fancyapps/fancybox": "^3.0.48",
    "codemirror": "^5.25.0",
    "jquery": "^3.2.1",
    "jquery-ui": "^1.12.1",
    "laravel-mix": "^0.10.0",
    "medium-editor": "^5.23.0",
    "sweetalert": "^1.1.3"
  }
